{
  "name": "@drrrs/ngx-numeric-textbox",
  "version": "1.0.6",
  "license": "MIT",
  "description": "An Angular numeric textbox component",
  "scripts": {
    "postinstall": "ngcc",
    "start": "ng serve",
    "lint": "ng lint",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "test-coverage": "ng test --watch=false --code-coverage",
    "e2e": "ng e2e",
    "ci": "npm run lint && npm run test-coverage && npm run e2e",
    "prebuild:lib": "rimraf tmp dist/lib",
    "build:lib": "gulp inline-templates && ngc -p tsconfig-lib.json && copyfiles package.json README.md LICENSE dist/lib",
    "postbuild:lib": "copyfiles -f dist/lib/src/tmp/* dist/lib/src && rimraf dist/lib/src/tmp",
    "prebuild:demo": "rimraf dist/demo",
    "build:demo": "ng build --prod",
    "pub:lib": "npm run build:lib && npm publish dist/lib",
    "pub:demo": "npm run build:demo -- --base-href /ngx-numeric-textbox/ && gh-pages -d dist/demo",
    "pub": "npm run pub:lib && npm run pub:demo"
  },
  "keywords": [
    "ng",
    "ngx",
    "angular",
    "numeric",
    "textbox",
    "ngx-numeric-textbox",
    "angular numeric textbox component"
  ],
  "author": "dur00s <mmertas@seznam.cz>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dur00s/ngx-numeric-textbox"
  },
  "bugs": {
    "url": "https://github.com/dur00s/ngx-numeric-textbox/issues"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "types": "src/index.d.ts",
  "private": false,
  "dependencies": {
    "@angular/animations": "^12.0.3",
    "@angular/core": "^12.0.3",
    "@angular/common": "^12.0.3",
    "@angular/compiler": "^12.0.3",
    "@angular/compiler-cli": "^12.0.3",
    "@angular/forms": "^12.0.3",
    "@angular/language-service": "^12.0.3",
    "@angular/platform-browser": "^12.0.3",
    "@angular/platform-browser-dynamic": "^12.0.3",
    "tslib": "2.2.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.1",
    "@angular/cli": "^12.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "12.11.1",
    "bootstrap": "4.5.3",
    "codelyzer": "6.0.1",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.0",
    "gulp-inline-ng2-template": "^5.0.1",
    "jasmine-core": "~2.6.2",
    "eslint": "^7.26.0",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "numeral": "^2.0.6",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "rxjs": "^6.6.3",
    "ts-node": "~3.2.0",
    "tslint": "6.1.3",
    "typescript": "4.2.4",
    "zone.js": "^0.10.3"
  }
}
